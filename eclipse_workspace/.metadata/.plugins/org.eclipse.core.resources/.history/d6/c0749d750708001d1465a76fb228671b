package com.codewithmosh;

import java.util.Date;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String msg = "testing";
		System.out.println(msg);
		int len = msg.length();
		
		if (len % 2 == 0) {
			System.out.println(msg.substring(len/2, len/2 +2));
		}
		else{
			System.out.println(msg.substring(len/2, len/2 +1));
		}
	}
}